{
  "name": "font-atlas-gui",
  "version": "1.0.0",
  "description": "a gui for font-atlas",
  "browser": "index.html",
  "dependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/css-font-loading-module": "0.0.4",
    "@types/node": "^14.0.27",
    "@types/webfontloader": "^1.6.32",
    "@types/workbox-sw": "^4.3.1",
    "css-font": "^1.2.0",
    "promise-file-reader": "^1.0.2",
    "webfontloader": "^1.6.28",
    "workbox-window": "^5.1.3"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "sass": "^1.26.10",
    "typescript": "^3.9.7",
    "uglify-es": "^3.3.9",
    "workbox-cli": "^5.1.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:sass": "npx --no-install sass --no-source-map --style compressed scss/:build/css/",
    "build:typescript": "npx --no-install tsc --build",
    "build:browserify": "npx --no-install browserify build/js/main.js -o build/js/app.js",
    "build:workbox": "npx --no-install workbox generateSW",
    "build:uglify": "npx --no-install uglifyjs build/js/app.js -o build/js/app.js --compress",
    "build": "npm run build:sass && npm run build:typescript && npm run build:browserify && npm run build:workbox && npm run build:uglify"
  },
  "author": "Lucide",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucide/font-atlas-gui.git"
  },
  "bugs": {
    "url": "https://github.com/Lucide/font-atlas-gui/issues"
  },
  "homepage": "https://github.com/Lucide/font-atlas-gui#readme"
}
